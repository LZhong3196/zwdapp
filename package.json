{
  "name": "zwdapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "debug": "node --harmony ./devtools/index.js debug",
    "debug-framework": "node --harmony ./devtools/index.js debug framework",
    "compile-framework": "node --harmony ./devtools/index.js compile framework",
    "test": "jest",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && npm cache clean && npm i"
  },
  "dependencies": {
    "@expo/vector-icons": "5.0.0",
    "events": "1.1.1",
    "immutable": "3.8.1",
    "lodash": "4.17.4",
    "mockjs": "1.0.1-beta2",
    "native-base": "2.2.0",
    "prop-types": "15.5.10",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.2",
    "react-native-fs": "2.3.3",
    "react-native-swiper": "^1.5.4",
    "react-native-image-crop-picker": "^0.14.2",
    "react-native-img-cache": "^1.4.0",
    "react-navigation": "1.0.0-beta.11",
    "react-redux": "5.0.5",
    "redux": "3.7.1",
    "redux-immutable": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-persist": "4.8.2",
    "redux-persist-immutable": "4.3.0",
    "redux-thunk": "2.2.0",
    "villa": "0.2.9"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.31",
    "@types/immutable": "^3.8.7",
    "@types/jest": "^20.0.2",
    "@types/lodash": "^4.14.67",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.31",
    "@types/react-native": "^0.44.12",
    "@types/react-native-fs": "^2.0.3",
    "@types/react-native-swiper": "^1.5.7",
    "@types/react-navigation": "^1.0.12",
    "@types/react-redux": "^4.4.45",
    "@types/react-test-renderer": "^15.5.1",
    "@types/redux": "^3.6.0",
    "@types/redux-immutable": "^3.0.34",
    "@types/redux-logger": "^3.0.0",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-thunk": "^2.1.0",
    "babel-jest": "20.0.3",
    "chalk": "1.1.3",
    "chokidar": "^1.7.0",
    "fs-extra": "^3.0.1",
    "minimatch": "^3.0.4",
    "react-native-debugger-open": "^0.3.11",
    "react-native-fetch-blob": "^0.10.6",
    "react-test-renderer": "16.0.0-alpha.12",
    "redux-devtools": "^3.4.0",
    "remote-redux-devtools": "^0.5.12",
    "request": "^2.81.0",
    "thenfail": "^0.4.13",
    "ts-jest": "^20.0.6",
    "typescript": "^2.3.4",
    "x-dts-generator": "^1.0.7"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}
